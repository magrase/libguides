<!-- Persistent accordion code  --><script>
$( document ).ready(function() {
    var lastPanel = getCookie("shl-panel-home");
    if (lastPanel != 'portal-menu-1'){
        $("#"+lastPanel+" h2 a").trigger("click");
    }
});

$(".panel.panel-default .panel-heading h2 a").click(function(){
	var id = $(this).parent().parent().attr('id');
	console.log("panel id="+id);
	document.cookie = "shl-panel-home="+id;
});


function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
</script>
